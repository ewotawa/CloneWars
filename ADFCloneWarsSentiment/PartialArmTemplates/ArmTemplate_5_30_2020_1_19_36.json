{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFCloneWarsSentiment"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/hashtags_dataset')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ewtwittersentiment_linkedservice",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "Json",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": {
							"value": "*.json",
							"type": "Expression"
						},
						"container": "countsbycharacter"
					}
				},
				"schema": {
					"type": "object",
					"properties": {
						"SystemTimeStamp": {
							"type": "string"
						},
						"Sentiment": {
							"type": "string"
						},
						"SentimentCount": {
							"type": "integer"
						},
						"SentimentScore": {
							"type": "number"
						},
						"AhsokaTano": {
							"type": "integer"
						},
						"AnakinSkywzalker": {
							"type": "integer"
						},
						"BoKatan": {
							"type": "integer"
						},
						"Maul": {
							"type": "integer"
						},
						"ObiWanKenobi": {
							"type": "integer"
						},
						"CaptainRex": {
							"type": "integer"
						},
						"BabyYoda": {
							"type": "integer"
						},
						"SiegeOfMandalore": {
							"type": "integer"
						},
						"StarWars": {
							"type": "integer"
						},
						"CloneWars": {
							"type": "integer"
						},
						"TheMandalorian": {
							"type": "integer"
						},
						"AhsokaLives": {
							"type": "integer"
						},
						"Jedi": {
							"type": "integer"
						},
						"Sith": {
							"type": "integer"
						},
						"MayThe4th": {
							"type": "integer"
						}
					}
				}
			},
			"dependsOn": []
		}
	]
}