{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFCloneWarsSentiment"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline_hashtags')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Hashtags_BlobToSql",
						"type": "Copy",
						"dependsOn": [
							{
								"activity": "Stored Procedure - Truncate Hashtags",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "JsonSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true
								}
							},
							"sink": {
								"type": "AzureSqlSink"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"path": "$['SystemTimeStamp']"
										},
										"sink": {
											"name": "SystemTimeStamp",
											"type": "DateTime"
										}
									},
									{
										"source": {
											"path": "$['Sentiment']"
										},
										"sink": {
											"name": "Sentiment",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "$['SentimentCount']"
										},
										"sink": {
											"name": "SentimentCount",
											"type": "Int32"
										}
									},
									{
										"source": {
											"path": "$['SentimentScore']"
										},
										"sink": {
											"name": "SentimentScore",
											"type": "Double"
										}
									},
									{
										"source": {
											"path": "$['AhsokaTano']"
										},
										"sink": {
											"name": "AhsokaTano",
											"type": "Int32"
										}
									},
									{
										"source": {
											"path": "$['AnakinSkywzalker']"
										},
										"sink": {
											"name": "AnakinSkywalker",
											"type": "Int32"
										}
									},
									{
										"source": {
											"path": "$['BoKatan']"
										},
										"sink": {
											"name": "BoKatan",
											"type": "Int32"
										}
									},
									{
										"source": {
											"path": "$['Maul']"
										},
										"sink": {
											"name": "Maul",
											"type": "Int32"
										}
									},
									{
										"source": {
											"path": "$['ObiWanKenobi']"
										},
										"sink": {
											"name": "ObiWanKenobi",
											"type": "Int32"
										}
									},
									{
										"source": {
											"path": "$['CaptainRex']"
										},
										"sink": {
											"name": "CaptainRex",
											"type": "Int32"
										}
									},
									{
										"source": {
											"path": "$['BabyYoda']"
										},
										"sink": {
											"name": "BabyYoda",
											"type": "Int32"
										}
									},
									{
										"source": {
											"path": "$['SiegeOfMandalore']"
										},
										"sink": {
											"name": "SiegeOfMandalore",
											"type": "Int32"
										}
									},
									{
										"source": {
											"path": "$['StarWars']"
										},
										"sink": {
											"name": "StarWars",
											"type": "Int32"
										}
									},
									{
										"source": {
											"path": "$['CloneWars']"
										},
										"sink": {
											"name": "CloneWars",
											"type": "Int32"
										}
									},
									{
										"source": {
											"path": "$['TheMandalorian']"
										},
										"sink": {
											"name": "TheMandalorian",
											"type": "Int32"
										}
									},
									{
										"source": {
											"path": "$['AhsokaLives']"
										},
										"sink": {
											"name": "AhsokaLives",
											"type": "Int32"
										}
									},
									{
										"source": {
											"path": "$['Jedi']"
										},
										"sink": {
											"name": "Jedi",
											"type": "Int32"
										}
									},
									{
										"source": {
											"path": "$['Sith']"
										},
										"sink": {
											"name": "Sith",
											"type": "Int32"
										}
									},
									{
										"source": {
											"path": "$['MayThe4th']"
										},
										"sink": {
											"name": "MayThe4th",
											"type": "Int32"
										}
									}
								]
							}
						},
						"inputs": [
							{
								"referenceName": "Hashtags_dataset",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "hashtags_sink",
								"type": "DatasetReference"
							}
						]
					},
					{
						"name": "Stored Procedure - Truncate Hashtags",
						"type": "SqlServerStoredProcedure",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"storedProcedureName": "[[dbo].[truncate_hashtags]"
						},
						"linkedServiceName": {
							"referenceName": "CloneWars_database_linkedservice",
							"type": "LinkedServiceReference"
						}
					}
				],
				"annotations": []
			},
			"dependsOn": []
		}
	]
}